# locale
export LANG=en_US.UTF-8

# paths
export PATH="$HOME/.local/bin:$PATH"

# static GPG home
export GNUPGHOME="$HOME/.gnupg"

# editor and pager
export EDITOR=nvim
export VISUAL=nvim
export PAGER=less

# git editor and pager
export GIT_EDITOR=nano
export GIT_PAGER=less

# less options
export LESS='-R'
